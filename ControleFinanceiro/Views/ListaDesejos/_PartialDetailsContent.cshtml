@model ControleFinanceiro.Models.ListaDesejo

<div class="content">
	<div class="row">
		<div class="form-group col-md-12">
			<label asp-for="DesejoId" class="control-label hidden"></label>
			<br />
			<div class="input-group col-md-12">
				<div class="input-group-prepend">
					<span class="input-group-text">
						<i class="fa fa-key title" aria-hidden="true"></i>
					</span>
				</div>
				<input asp-for="DesejoId" class="form-control" disabled="disabled" />
			</div>
			<label asp-for="DesejoNome" class="control-label hidden"></label>
			<br />
			<div class="input-group col-md-12">
				<div class="input-group-prepend">
					<span class="input-group-text">
						<i class="fa fa-user-circle title" aria-hidden="true"></i>
					</span>
				</div>
				<input asp-for="DesejoNome" class="form-control" disabled="disabled" />
			</div>
			<label asp-for="DesejoDescricao" class="control-label hidden"></label>
			<br />
			<div class="input-group col-md-12">
				<div class="input-group-prepend">
					<span class="input-group-text">
						<i class="fa fa-edit title" aria-hidden="true"></i>
					</span>
				</div>
				<input asp-for="DesejoDescricao" class="form-control" disabled="disabled" />
			</div>
			<label asp-for="DesejoValor" class="control-label  hidden"></label>
			<br />
			<div class="input-group col-md-12">
				<div class="input-group-prepend">
					<span class="input-group-text">
						<i class="fa fa-list-alt title" aria-hidden="true"></i>
					</span>
				</div>
				<input asp-for="DesejoValor" class="form-control" disabled="disabled" />
			</div>
			<label asp-for="FormaPagamento.FormaNome" class="control-label  hidden"></label>
			<br />
			<div class="input-group col-md-12">
				<div class="input-group-prepend">
					<span class="input-group-text">
						<i class="fa fa-list-alt title" aria-hidden="true"></i>
					</span>
				</div>
				<input asp-for="FormaPagamento.FormaNome" class="form-control" disabled="disabled" />
			</div>
			<label asp-for="DesejoData" class="control-label  hidden"></label>
			<br />
			<div class="input-group col-md-12">
				<div class="input-group-prepend">
					<span class="input-group-text">
						<i class="fa fa-list-alt title" aria-hidden="true"></i>
					</span>
				</div>
				<input asp-for="DesejoData" class="form-control" disabled="disabled" />
			</div>
			<div class="form-group text-left h3">
				<input type="hidden" asp-for="DesejoId" />
				<a asp-action="Edit" class="btn btn-block btn-warning btn-fill" asp-route-id="@Model.DesejoId">Editar</a>
				<a asp-action="Index" class="btn btn-block btn-info btn-fill pull-right">Voltar</a>
			</div>
		</div>
	</div>
</div>